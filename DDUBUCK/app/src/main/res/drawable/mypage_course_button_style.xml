<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/next_time_dialog_style">
        <shape android:shape="rectangle">
            <corners android:radius="10dip" />
            <gradient
                android:angle="180"
                android:startColor="#57A01D"
                android:endColor="#589627"
                android:type="linear" />
            <stroke
                android:width="0dp"
                android:color="#FFFFFF" />
        </shape>
    </item>
</selector>